-- BORRAR TABLAS DE MENSAJES WEBSERVICE POR OPERACION
DELETE FROM SYSADM.PSIBLOGHDR
 WHERE GUID IN (SELECT GUID
                  FROM SYSADM.PSIBLOGHDR
                 WHERE IB_OPERATIONNAME = 'WS_CLIENTECREACION');
DELETE FROM SYSADM.PSIBLOGDATA
 WHERE GUID IN (SELECT GUID
                  FROM SYSADM.PSIBLOGHDR
                 WHERE IB_OPERATIONNAME = 'WS_CLIENTECREACION');
DELETE FROM SYSADM.PSIBLOGERR
 WHERE GUID IN (SELECT GUID
                  FROM SYSADM.PSIBLOGHDR
                 WHERE IB_OPERATIONNAME = 'WS_CLIENTECREACION');
DELETE FROM SYSADM.PSIBLOGERRP
 WHERE GUID IN (SELECT GUID
                  FROM SYSADM.PSIBLOGHDR
                 WHERE IB_OPERATIONNAME = 'WS_CLIENTECREACION');
DELETE FROM SYSADM.PSIBLOGIBINFO
 WHERE GUID IN (SELECT GUID
                  FROM SYSADM.PSIBLOGHDR
                 WHERE IB_OPERATIONNAME = 'WS_CLIENTECREACION');
DELETE FROM SYSADM.PSIBLOGIBINFO
 WHERE GUID IN (SELECT GUID
                  FROM SYSADM.PSIBLOGHDR
                 WHERE IB_OPERATIONNAME = 'WS_CLIENTECREACION');
                 
COMMIT;                 
